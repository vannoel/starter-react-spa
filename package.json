{
  "name": "starter-react-spa",
  "version": "1.0.0",
  "description": "A starter for single-page application of website based on React.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "clean-modules": "rimraf node_modules",
    "build": "npm run build-prod",
    "build-dev": "webpack --progress --color --config webpack.dev.js",
    "build-prod": "webpack --progress --color --config webpack.prod.js",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --open --devtool eval --progress --color --content-base build --config webpack.dev.js",
    "start-prod": "webpack-dev-server --open --devtool eval --progress --color --content-base build --config webpack.prod.js",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "eslint --ext .js,.jsx .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vannoel/starter-react-spa.git"
  },
  "author": "NoelChang",
  "license": "MIT",
  "homepage": "https://github.com/vannoel/starter-react-spa#readme",
  "devDependencies": {
