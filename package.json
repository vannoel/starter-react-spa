    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
